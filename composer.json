{
    "name": "chanshige/laravel-query-logger",
    "description": "A Laravel database queries logger provider.",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Shigeki Tanaka",
            "email": "dev@shigeki.tokyo"
        }
    ],
    "autoload": {
        "psr-4": {
            "Chanshige\\Laravel\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Chanshige\\Laravel\\": "tests/"
        }
    },
    "require": {
        "php": "^7.3.11",
        "illuminate/contracts": "^7.0",
        "illuminate/database": "^7.0",
        "illuminate/log": "^7.0",
        "illuminate/support": "^7.0",
        "psr/log": "^1.1"
    },
    "require-dev": {
        "doctrine/coding-standard": "^8.1",
        "friendsofphp/php-cs-fixer": "^2.16",
        "squizlabs/php_codesniffer": "^3.5",
        "phpmd/phpmd": "^2.8",
        "phpunit/phpunit": "^9.3",
        "mockery/mockery": "^1.4"
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "extra": {
        "laravel": {
            "providers": [
                "Chanshige\\Laravel\\QueryLoggerProvider"
            ]
        }
    },
    "scripts": {
        "tests": [
            "phpmd src text ./phpmd.xml",
            "phpcs src",
            "@cs",
            "@test"
        ],
        "test": [
            "phpunit"
        ],
        "cs": [
            "php-cs-fixer fix --dry-run --diff --diff-format udiff ./src"
        ]
    }
}
