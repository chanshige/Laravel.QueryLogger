{
    "name": "chanshige/laravel-query-logger",
    "description": "A Laravel database queries logger provider.",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Shigeki Tanaka",
            "email": "dev@shigeki.tokyo"
        }
    ],
    "autoload": {
        "psr-4": {
            "Chanshige\\Laravel\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Chanshige\\Laravel\\": "tests/"
        }
    },
    "require": {
        "php": "^7.3|^8.0",
        "psr/log": "^1.1|^3.0",
        "illuminate/contracts": "^6.0|^7.0|^8.0|^9.x-dev",
        "illuminate/database": "^6.0|^7.0|^8.0|^9.x-dev",
        "illuminate/log": "^6.0|^7.0|^8.0|^9.x-dev",
        "illuminate/support": "^6.0|^7.0|^8.0|^9.x-dev",
        "illuminate/config": "^6.0|^7.0|^8.0|^9.x-dev",
        "illuminate/events": "^6.0|^7.0|^8.0|^9.x-dev",
        "illuminate/filesystem": "^6.0|^7.0|^8.0|^9.x-dev"
    },
    "require-dev": {
        "doctrine/coding-standard": "^8.1",
        "squizlabs/php_codesniffer": "^3.6",
        "phpmd/phpmd": "^2.10",
        "phpunit/phpunit": "^9.3"
    },
    "extra": {
        "laravel": {
            "providers": [
                "Chanshige\\Laravel\\QueryLoggerProvider"
            ]
        }
    },
    "scripts": {
        "tests": [
            "phpmd src text ./phpmd.xml",
            "phpcs src",
            "@test"
        ],
        "test": [
            "phpunit"
        ]
    },
    "config": {
        "sort-packages": true
    },
    "prefer-stable": true,
    "minimum-stability": "dev"
}
